
<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>

<%= form_tag url: @url, method: 'post' do %>
 <h1>Questions:</h1>
   <% @questions.each do |question| %>
     <h1><%= question.text %></h1>
     
       <% question.options.each do |option| %>
       <p>
         <%= radio_button_tag(question.id, option.id) %>
         <%= label_tag(:option, option.text) %>
       </p>
     <% end %>
   <% end %>
  <%= submit_tag %>
<% end %>